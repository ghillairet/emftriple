<plugin
      name="%pluginName"
      id="com.emftriple.examples.basic"
      version="1.0.0"
      provider-name="%providerName">

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
   </requires>

   <runtime>
      <library name=".">
         <export name="*"/>
      </library>
   </runtime>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipselabs.org/emftriple/model"
            class="com.emftriple.examples.basic.model.ModelPackage"
            genModel="model/model.genmodel"/>
   </extension>

</plugin>
